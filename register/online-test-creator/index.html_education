<script>
	var regFormSubmitted = false;
	var web_root = '/';
	$(document).ready(function() {

		$('#email').change(function(){
			$('#checkEmail').show();
			$("#copy_" + this.id).text($('#' + this.id).val());
		});
		$('#email').keyup(function(){
			$('#checkEmail').show();
			$("#copy_" + this.id).text($('#' + this.id).val());
		});

		/* Instructor reg form */
		$('#regForm').submit(function()  {

			form_completed = true;
			$("#regForm").find("input[type=text]").each(function() {
				if (form_completed && $(this).val().length == 0 && $(this).attr('name') != 'display_name' ) {//display_name NOT REQUIRED
					alert( 'All Mandatory * fields are required' );
					form_completed = false;
				}
			});
			if ( ! $('#agree-to-terms').is(':checked')) {
				alert('You must agree to our Terms and Conditions and Privacy Policy to Register.');
				form_completed = false;
			}

			if (form_completed && !regFormSubmitted) {
				regFormSubmitted = true;
				return true;
			} else {
				return false;
			}
		});

		
	});
</script>
